generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}

model Student {
  id          String       @id @default(uuid())
  studentId   String       @unique
  name        String?
  age         Int?
  gender      String?
  state       String?
  createdAt   DateTime     @default(now())
  enrollments Enrollment[]
}

model Enrollment {
  id        String  @id @default(uuid())
  studentId String
  term      String
  program   String
  credits   Float?
  status    String?
  student   Student @relation(fields: [studentId], references: [studentId])
}
